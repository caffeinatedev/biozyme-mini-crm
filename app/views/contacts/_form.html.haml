= form_with model: @contact, local: true do |f|
  .mb-3
    = f.label :name
    = f.text_field :name, class: "form-control"

  .mb-3
    = f.label :company
    = f.text_field :company, class: "form-control"

  .mb-3
    = f.label :email
    = f.email_field :email, class: "form-control"

  = f.submit class: "btn btn-primary"
